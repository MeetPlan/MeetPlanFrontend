{
  "name": "meetplan",
  "version": "1.0.0",
  "private": true,
  "package": "module",
  "type": "module",
  "scripts": {
    "build": "vite build --outDir public && npm run prep",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "dev": "vite --port 8080",
    "start": "sirv public --no-clear -s -G",
    "prep": "npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme-light": "smui-theme compile public/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile public/smui-dark.css -i src/theme/dark"
  },
  "devDependencies": {
    "@material/elevation": "^14.0.0",
    "@material/theme": "^14.0.0",
    "@material/typography": "^14.0.0",
    "@smui-extra/accordion": "^7.0.0",
    "@smui-extra/autocomplete": "^7.0.0",
    "@smui-extra/badge": "^7.0.0",
    "@smui/button": "^7.0.0",
    "@smui/card": "^7.0.0",
    "@smui/checkbox": "^7.0.0",
    "@smui/chips": "^7.0.0",
    "@smui/circular-progress": "^7.0.0",
    "@smui/common": "^7.0.0",
    "@smui/data-table": "^7.0.0",
    "@smui/dialog": "^7.0.0",
    "@smui/drawer": "^7.0.0",
    "@smui/form-field": "^7.0.0",
    "@smui/icon-button": "^7.0.0",
    "@smui/linear-progress": "^7.0.0",
    "@smui/list": "^7.0.0",
    "@smui/menu-surface": "^7.0.0",
    "@smui/paper": "^7.0.0",
    "@smui/radio": "^7.0.0",
    "@smui/ripple": "^7.0.0",
    "@smui/segmented-button": "^7.0.0",
    "@smui/select": "^7.0.0",
    "@smui/slider": "^7.0.0",
    "@smui/snackbar": "^7.0.0",
    "@smui/switch": "^7.0.0",
    "@smui/textfield": "^7.0.0",
    "@smui/tooltip": "^7.0.0",
    "@smui/top-app-bar": "^7.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "@tsconfig/svelte": "^5.0.4",
    "date-fns": "^3.6.0",
    "insane": "^2.6.2",
    "is-mobile": "^4.0.0",
    "marked": "^14.0.0",
    "sirv-cli": "^2.0.2",
    "smui-theme": "^7.0.0",
    "svelte": "^4.2.18",
    "svelte-avatar": "^1.2.0",
    "svelte-check": "^3.8.5",
    "svelte-dnd-action": "^0.9.49",
    "svelte-material-ui": "^7.0.0",
    "svelte-routing": "^2.13.0",
    "tslib": "^2.6.3",
    "typescript": "^5.5.4",
    "vite": "^5.4.0",
    "vite-plugin-compression": "^0.5.1"
  }
}
