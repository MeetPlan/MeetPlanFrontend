{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "rollup -c",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "dev": "rollup -c -w",
    "start": "sirv public --no-clear -s",
    "tauri": "tauri",
    "tauri-build": "TAURI=true tauri build",
    "tauri-dev": "TAURI=true tauri dev",
    "prepare": "npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme-light": "smui-theme compile public/build/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile public/build/smui-dark.css -i src/theme/dark"
  },
  "devDependencies": {
    "@material/elevation": "^14.0.0",
    "@material/theme": "^14.0.0",
    "@material/typography": "^14.0.0",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "@smui-extra/accordion": "^6.0.0-beta.16",
    "@smui-extra/autocomplete": "^6.0.0-beta.16",
    "@smui-extra/badge": "^6.0.0-beta.16",
    "@smui/button": "^6.0.0-beta.16",
    "@smui/common": "^6.0.0-beta.16",
    "@smui/data-table": "^6.0.0-beta.16",
    "@smui/dialog": "^6.0.0-beta.16",
    "@smui/drawer": "^6.0.0-beta.16",
    "@smui/list": "^6.0.0-beta.16",
    "@smui/menu-surface": "^6.0.0-beta.16",
    "@smui/paper": "^6.0.0-beta.16",
    "@smui/ripple": "^6.0.0-beta.16",
    "@smui/select": "^6.0.0-beta.16",
    "@smui/snackbar": "^6.0.0-beta.16",
    "@smui/textfield": "^6.0.0-beta.16",
    "@smui/tooltip": "^6.0.0-beta.16",
    "@tauri-apps/api": "^1.0.1",
    "@tauri-apps/cli": "^1.0.5",
    "@tsconfig/svelte": "^3.0.0",
    "@types/marked": "^4.0.3",
    "apexcharts": "^3.35.3",
    "date-fns": "^2.28.0",
    "insane": "^2.6.2",
    "is-mobile": "^3.1.1",
    "jwt-decode": "^3.1.2",
    "marked": "^4.0.17",
    "rollup": "^2.75.7",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "sirv-cli": "^2.0.2",
    "smui-theme": "^6.0.0-beta.16",
    "svelte": "^3.49.0",
    "svelte-apexcharts": "^1.0.2",
    "svelte-avatar": "^1.1.1",
    "svelte-check": "^2.8.0",
    "svelte-dnd-action": "^0.9.18",
    "svelte-material-ui": "^6.0.0-beta.16",
    "svelte-preprocess": "^4.10.7",
    "svelte-routing": "^1.6.0",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  }
}
